{
    "name": "natpnk/transip-laravel",
    "description": "A Laravel wrapper for the TransIP REST API Client.",
    "require": {
        "php": "^8.0",
        "illuminate/support": "^9.0|10.0|^11.0|^12.0",
        "transip/transip-api-php": "^6.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Natpnk\\TransIPLaravel\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Natpnk\\TransIPLaravel\\TransIPServiceProvider"
            ],
            "aliases": {
                "TransIP": "Natpnk\\TransIPLaravel\\TransIPFacade"
            }
        }
    },
    "authors": [
        {
            "name": "Natpnk",
            "email": "patrick@natpnk.nl"
        }
    ]
}
